{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "xiv-data",
  "title": "XIV Data",
  "description": "Look up wiki articles and prices",
  "icon": "command-icon.png",
  "author": "markus_becker",
  "categories": [
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "XIV Lookup",
      "description": "Searches for XIV data using XIVAPI, links to Universalis.app and ffxiv community wiki",
      "mode": "view",
      "arguments": [
        {
          "name": "search",
          "placeholder": "Search Term",
          "type": "text"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.54.1",
    "@raycast/utils": "^1.8.0",
    "@types/axios": "^0.14.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
